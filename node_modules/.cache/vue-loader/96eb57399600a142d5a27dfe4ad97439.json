nd (min-width: 320px) and (max-width: 1600px) {\n    &__row {\n      &_direction {\n        flex-direction: column;\n        align-items: start;\n      }\n    }\n  }\n  @media screen and (min-width: 744px) and (max-width: 1280px) {\n    &__title {\n      font-size: 24px;\n    }\n    &__name,\n    &__subtitle {\n      font-size: 16px;\n    }\n    &__parameter,\n    &__value,\n    &__status {\n      font-size: 14px;\n    }\n  }\n  @media screen and (min-width: 320px) and (max-width: 850px) {\n    &__months {\n      width: 100%;\n    }\n    &__months .main-filter {\n      width: 795px;\n    }\n  }\n  @media screen and (min-width: 320px) and (max-width: 744px) {\n    &__name {\n      font-size: 14px;\n    }\n    &__parameter,\n    &__value,\n    &__status {\n      font-size: 12px;\n    }\n    &__column {\n      padding: 4px;\n    }\n    &__title {\n      font-size: 16px;\n    }\n  }\n}\n</style>"]}]}